  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#546e7a"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#e1f5fe"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff6b6b"
      }
    },
    {
      "name": "Control Keywords",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#ff8a65"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "meta.function-call", "variable.function", "support.function"],
      "settings": {
        "foreground": "#4ecdc4"
      }
    },
    {
      "name": "Classes & Types",
      "scope": ["entity.name.class", "entity.name.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#45b7d1"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#96ceb4"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#feca57"
      }
    },
    {
      "name": "Booleans & Constants",
      "scope": ["constant.language", "constant.character"],
      "settings": {
        "foreground": "#ff9ff3"
      }
    },
    {
      "name": "Properties",
      "scope": ["variable.other.property", "support.type.property-name", "entity.other.attribute-name"],
      "settings": {
        "foreground": "#54a0ff"
      }
    },
    {
      "name": "Methods",
      "scope": ["entity.name.method", "variable.function.constructor"],
      "settings": {
        "foreground": "#5f27cd"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ff3838"
      }
    },
    {
      "name": "Tag Attributes",
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#ff6348"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#ff7675"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#a4b0be"
      }
    },
    {
      "name": "Imports",
      "scope": ["keyword.control.import", "keyword.control.from"],
      "settings": {
        "foreground": "#00d2d3"
      }
    },
    {
      "name": "Decorators",
      "scope": ["entity.name.function.decorator", "punctuation.decorator"],
      "settings": {
        "foreground": "#ff9f43"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#ff6b81"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#70a1ff"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#7bed9f"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": ["entity.name.tag.css", "entity.other.attribute-name.class.css"],
      "settings": {
        "foreground": "#2ed573"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#3742fa"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#f1c40f"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#ff4757",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#ffa502",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#3c40c6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#05c46b"
      }
    },
    {
      "name": "Invalid/Error",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff3838",
        "fontStyle": "underline"
      }
    }
  ]
}
