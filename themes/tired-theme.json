{
  "name": "Rizz Tired",
  "type": "dark",
  "colors": {
    "editor.background": "#1a1625",
    "editor.foreground": "#f5f3ff",
    "activityBar.background": "#2d2438",
    "activityBar.foreground": "#a78bfa",
    "sideBar.background": "#2d2438",
    "sideBar.foreground": "#d1d5db",
    "statusBar.background": "#ec4899",
    "statusBar.foreground": "#ffffff",
    "tab.activeBackground": "#1a1625",
    "tab.activeBorder": "#a78bfa",
    "tab.activeForeground": "#f5f3ff",
    "tab.inactiveBackground": "#2d2438",
    "tab.inactiveForeground": "#9ca3af",
    "editor.selectionBackground": "#a78bfa60",
    "editor.lineHighlightBackground": "#2d243840",
    "editorCursor.foreground": "#ec4899",
    "editorLineNumber.foreground": "#9ca3af",
    "editorLineNumber.activeForeground": "#a78bfa",
    "editorSuggestWidget.background": "#3d3451",
    "editorSuggestWidget.border": "#fbbf24",
    "editorSuggestWidget.foreground": "#f5f3ff",
    "editorSuggestWidget.highlightForeground": "#ec4899",
    "editorSuggestWidget.selectedBackground": "#fbbf2440",
    "editorSuggestWidget.selectedForeground": "#ffffff",
    "editorGhostText.foreground": "#fbbf24",
    "editorGhostText.background": "#fbbf2420",
    "editorInlayHint.foreground": "#22d3ee",
    "editorInlayHint.background": "#22d3ee20",
    "editorCodeLens.foreground": "#c084fc"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c084fc"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#22d3ee"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ec4899",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator, Punctuation",
      "scope": ["keyword.control", "punctuation", "meta.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#a78bfa"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml"],
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function-call", "variable.function", "support.function"],
      "settings": {
        "foreground": "#fbbf24",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Number, Constant",
      "scope": ["constant.numeric", "constant.language", "support.constant", "constant.character"],
      "settings": {
        "foreground": "#fb7185"
      }
    },
    {
      "name": "String",
      "scope": ["string", "constant.other.symbol", "constant.other.key"],
      "settings": {
        "foreground": "#34d399"
      }
    },
    {
      "name": "Class, Support",
      "scope": ["entity.name", "support.type", "support.class"],
      "settings": {
        "foreground": "#06b6d4"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#6366f1"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#c084fc"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": ["text.html.basic entity.other.attribute-name.html"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#fde047"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#ec4899"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#34d399"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": ["markup.heading", "markup.heading.markdown"],
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ec4899"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#22d3ee"
      }
    }
  ]
}
