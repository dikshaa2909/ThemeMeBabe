{
  "name": "Rizz Focused ",
  "type": "dark",
  "colors": {
    "editor.background": "#2E003E",
    "editor.foreground": "#F9A8D4",
    "statusBar.background": "#7B2FF7",
    "statusBar.foreground": "#FFFFFF",
    "activityBar.background": "#9F5DE2",
    "sideBar.background": "#3A0CA3",
    "sideBar.foreground": "#F9A8D4",
    "tab.activeBackground": "#2E003E",
    "tab.activeBorder": "#00FF00",
    "tab.activeForeground": "#F9A8D4",
    "tab.inactiveBackground": "#3A0CA3",
    "tab.inactiveForeground": "#B794F6",
    "editor.selectionBackground": "#7B2FF740",
    "editor.lineHighlightBackground": "#3A0CA340",
    "editorCursor.foreground": "#00FF00",
    "editorLineNumber.foreground": "#B794F6",
    "editorLineNumber.activeForeground": "#F9A8D4",
    "editorSuggestWidget.background": "#3A0CA3",
    "editorSuggestWidget.border": "#00FF00",
    "editorSuggestWidget.foreground": "#FFFFFF",
    "editorSuggestWidget.highlightForeground": "#00FF00",
    "editorSuggestWidget.selectedBackground": "#00FF0040",
    "editorGhostText.foreground": "#00FF00",
    "editorInlayHint.foreground": "#FFFF00",
    "editorCodeLens.foreground": "#FF0080"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#666666",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier", "keyword.control"],
      "settings": {
        "foreground": "#FF0000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "string constant.other.placeholder", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#FFFF00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String",
      "scope": ["string", "constant.other.symbol", "constant.other.key", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric", "constant.language", "support.constant", "constant.character", "constant.escape"],
      "settings": {
        "foreground": "#FF8000"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class",
        "entity.name.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#8000FF"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "#FF0080"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name", "entity.other.attribute-name.html", "entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#00FF80"
      }
    },
    {
      "name": "Operator",
      "scope": ["punctuation", "meta.tag", "punctuation.definition.tag", "keyword.operator"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF4000",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "support.variable.property"],
      "settings": {
        "foreground": "#FF8080"
      }
    },
    {
      "name": "Method",
      "scope": ["entity.name.method", "variable.function.method"],
      "settings": {
        "foreground": "#80FF00"
      }
    },
    {
      "name": "Import/Export",
      "scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from"],
      "settings": {
        "foreground": "#0080FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css", "support.type.property-name.scss"],
      "settings": {
        "foreground": "#FF4080"
      }
    },
    {
      "name": "CSS Value",
      "scope": ["support.constant.property-value.css", "constant.other.color.rgb-value.css"],
      "settings": {
        "foreground": "#40FF80"
      }
    }
  ]
}
