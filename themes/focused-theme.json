    "editor.foreground": "#e8f5e8",
    "activityBar.background": "#1a2e1a",
    "activityBar.foreground": "#4ade80",
    "activityBar.inactiveForeground": "#4a5c4a",
    "activityBarBadge.background": "#4ade80",
    "activityBarBadge.foreground": "#ffffff",
    "badge.background": "#4ade80",
    "badge.foreground": "#ffffff",
    "button.background": "#4ade80",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#5eeb8f",
    "dropdown.background": "#1e3a1e",
    "dropdown.border": "#06b6d4",
    "dropdown.foreground": "#e8f5e8",
    "editor.findMatchBackground": "#4ade8055",
    "editor.findMatchHighlightBackground": "#4ade8033",
    "editor.lineHighlightBackground": "#1a2e1a",
    "editor.selectionBackground": "#06b6d444",
    "editor.selectionHighlightBackground": "#06b6d422",
    "editorBracketMatch.background": "#4ade8044",
    "editorBracketMatch.border": "#06b6d4",
    "editorCursor.foreground": "#06b6d4",
    "editorError.foreground": "#ef4444",
    "editorGroup.border": "#1e3a1e",
    "editorGroupHeader.tabsBackground": "#1a2e1a",
    "editorGutter.addedBackground": "#4ade80",
    "editorGutter.deletedBackground": "#ef4444",
    "editorGutter.modifiedBackground": "#eab308",
    "editorIndentGuide.background": "#1e3a1e",
    "editorIndentGuide.activeBackground": "#06b6d4",
    "editorLineNumber.foreground": "#4a5c4a",
    "editorLineNumber.activeForeground": "#06b6d4",
    "editorWidget.background": "#1e3a1e",
    "editorWidget.border": "#06b6d4",
    "focusBorder": "#06b6d4",
    "input.background": "#1e3a1e",
    "input.border": "#06b6d4",
    "input.foreground": "#e8f5e8",
    "list.activeSelectionBackground": "#4ade8033",
    "list.activeSelectionForeground": "#e8f5e8",
    "list.hoverBackground": "#06b6d422",
    "list.inactiveSelectionBackground": "#1e3a1e",
    "panel.background": "#1a2e1a",
    "panel.border": "#1e3a1e",
    "panelTitle.activeBorder": "#06b6d4",
    "panelTitle.activeForeground": "#e8f5e8",
    "panelTitle.inactiveForeground": "#4a5c4a",
    "scrollbarSlider.background": "#06b6d444",
    "scrollbarSlider.hoverBackground": "#06b6d466",
    "scrollbarSlider.activeBackground": "#06b6d488",
    "sideBar.background": "#1a2e1a",
    "sideBar.foreground": "#e8f5e8",
    "sideBarSectionHeader.background": "#0d1b0d",
    "sideBarSectionHeader.foreground": "#4ade80",
    "statusBar.background": "#0d1b0d",
    "statusBar.foreground": "#e8f5e8",
    "statusBar.debuggingBackground": "#4ade80",
    "statusBar.debuggingForeground": "#ffffff",
    "tab.activeBackground": "#0d1b0d",
    "tab.activeForeground": "#e8f5e8",
    "tab.activeBorder": "#06b6d4",
    "tab.inactiveBackground": "#1a2e1a",
    "tab.inactiveForeground": "#4a5c4a",
    "tab.border": "#1e3a1e",
    "terminal.ansiBlack": "#0d1b0d",
    "terminal.ansiRed": "#ef4444",
    "terminal.ansiGreen": "#4ade80",
    "terminal.ansiYellow": "#eab308",
    "terminal.ansiBlue": "#06b6d4",
    "terminal.ansiMagenta": "#a855f7",
    "terminal.ansiCyan": "#14b8a6",
    "terminal.ansiWhite": "#e8f5e8",
    "terminal.ansiBrightBlack": "#4a5c4a",
    "terminal.ansiBrightRed": "#f87171",
    "terminal.ansiBrightGreen": "#5eeb8f",
    "terminal.ansiBrightYellow": "#fde047",
    "terminal.ansiBrightBlue": "#22d3ee",
    "terminal.ansiBrightMagenta": "#c084fc",
    "terminal.ansiBrightCyan": "#2dd4bf",
    "terminal.ansiBrightWhite": "#ffffff",
    "titleBar.activeBackground": "#0d1b0d",
    "titleBar.activeForeground": "#e8f5e8",
    "titleBar.inactiveBackground": "#1a2e1a",
    "titleBar.inactiveForeground": "#4a5c4a"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4a5c4a"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#e8f5e8"
      }
    },
    {
      "name": "Keywords - Forest Power",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#4ade80",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control Keywords - Deep Ocean",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#14b8a6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Functions - Sky Blue",
      "scope": ["entity.name.function", "meta.function-call", "variable.function", "support.function"],
      "settings": {
        "foreground": "#06b6d4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Classes & Types - Electric Blue",
      "scope": ["entity.name.class", "entity.name.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#22d3ee",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings - Golden Glow",
      "scope": ["string"],
      "settings": {
        "foreground": "#eab308"
      }
    },
    {
      "name": "Numbers - Bright Orange",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#f97316",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Booleans & Constants - Pure White",
      "scope": ["constant.language", "constant.character"],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Properties - Teal Fresh",
      "scope": ["variable.other.property", "support.type.property-name", "entity.other.attribute-name"],
      "settings": {
        "foreground": "#14b8a6"
      }
    },
    {
      "name": "Methods - Lime Green",
      "scope": ["entity.name.method", "variable.function.constructor"],
      "settings": {
        "foreground": "#84cc16",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tags - Emerald Green",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#10b981",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag Attributes - Cyan Blue",
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#06b6d4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operators - Teal Power",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#14b8a6"
      }
    },
    {
      "name": "Punctuation - Soft Green",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#a7c7a7"
      }
    },
    {
      "name": "Imports - Ocean Blue",
      "scope": ["keyword.control.import", "keyword.control.from"],
      "settings": {
        "foreground": "#0ea5e9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Decorators - Amber Glow",
      "scope": ["entity.name.function.decorator", "punctuation.decorator"],
      "settings": {
        "foreground": "#f59e0b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Regular Expressions - Red Alert",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#ef4444"
      }
    },
    {
      "name": "Escape Characters - Mint Fresh",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#84cc16"
      }
    },
    {
      "name": "JSON Keys - Sky Blue",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#06b6d4"
      }
    },
    {
      "name": "CSS Selectors - Forest Green",
      "scope": ["entity.name.tag.css", "entity.other.attribute-name.class.css"],
      "settings": {
        "foreground": "#16a34a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Properties - Ocean Blue",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#06b6d4"
      }
    },
    {
      "name": "CSS Values - Golden Yellow",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#eab308"
      }
    },
    {
      "name": "Markdown Headings - Bright Green",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#22c55e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold - Orange Power",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#f97316",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic - Teal Flow",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#14b8a6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code - Sky Blue",
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#06b6d4"
      }
    },
    {
      "name": "Invalid/Error - Red Warning",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ef4444",
        "fontStyle": "underline"
      }
    }
  ]
}
